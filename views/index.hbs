<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Nest Application</title>
  </head>
  <body>
    <h1>This is my MVC nest application.</h1>

    {{#each books}}
        <h2>Title: {{this.title}}</h2>
        <h2>Author: {{this.author}}</h2>
        <h2>Publish: {{this.publish}}</h2>
    {{/each}}

  </body>
</html>